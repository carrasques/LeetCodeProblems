class Solution {
  public boolean isPalindrome(String s) {

      s = s.replaceAll("[^a-zA-Z0-9]+", "");

      for(int begin = 0, end = s.length()-1; end>=begin; end--, begin++) {
          if(Character.toLowerCase(s.charAt(begin)) != Character.toLowerCase(s.charAt(end)))
              return false;
      }

      return true;

  }
}
