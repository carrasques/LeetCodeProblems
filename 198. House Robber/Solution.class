class Solution {
    public int rob(int[] nums) {

        int excl = 0;
        int incl = 0;
        for(int i = 0; i < nums.length; i++) {
            int newExcl = Math.max(incl,excl);
            int newIncl = nums[i] + excl;
            excl = newExcl;
            incl = newIncl;
        }
        return Math.max(excl, incl);
    }
}
